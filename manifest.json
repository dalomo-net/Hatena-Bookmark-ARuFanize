{
    "name": "Hatena Bookmark ARuFanize",
    "version": "0.1",
    "description": "HatenaBookmark no kao ni mesen wo tsukeru.",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}